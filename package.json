{
  "name": "eslint-plugin-pimsical-shopify-graphql",
  "version": "0.0.0-dev",
  "description": "An ESLint plugin to help check Shopify GraphQL queries follow best practice. Built by a Shopify Partner.",
  "type": "module",
  "author": "jordanfinners",
  "license": "ISC",
  "main": "dist/plugin.js",
  "files": ["dist", "!**/*.test.ts"],
  "scripts": {
    "build": "tsc",
    "test": "vitest --globals=true",
    "lint": "eslint . && npm run typecheck",
    "lint:fix": "eslint . --fix && npm run typecheck",
    "typecheck": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jordanfinners/eslint-plugin-pimsical-shopify-graphql.git"
  },
  "bugs": {
    "url": "https://github.com/jordanfinners/eslint-plugin-pimsical-shopify-graphql/issues"
  },
  "homepage": "https://github.com/jordanfinners/eslint-plugin-pimsical-shopify-graphql#readme",
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin",
    "graphql",
    "shopify"
  ],
  "peerDependencies": {
    "@graphql-eslint/eslint-plugin": "^4.4.0",
		"eslint": ">=8.0.0"
	},
  "devDependencies": {
    "@eslint/js": "^9.4.0",
    "@graphql-eslint/eslint-plugin": "^4.4.0",
    "@types/eslint": "^9.6.0",
    "eslint": "^9.0.0",
    "eslint-plugin-eslint-plugin": "^6.2.0",
    "globals": "^16.0.0",
    "graphql": "^16.11.0",
    "typescript": "^5.4.5",
    "typescript-eslint": "^8.12.0",
    "vitest": "^3.0.0"
  }
}
